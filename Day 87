//Q137: Create an enum for user roles (ADMIN, USER, GUEST) and display messages based on role.
#include <stdio.h>
#include <string.h>
// Define enum for user roles
typedef enum {
    ADMIN,
    USER,
    GUEST,
    INVALID
} Role;
// Function to convert string to enum
Role getRole(char *roleStr) {
    if (strcmp(roleStr, "ADMIN") == 0) return ADMIN;
    else if (strcmp(roleStr, "USER") == 0) return USER;
    else if (strcmp(roleStr, "GUEST") == 0) return GUEST;
    else return INVALID;
}
int main() {
    char roleStr[20];
    scanf("%s", roleStr);
    Role role = getRole(roleStr);
    switch (role) {
        case ADMIN:
            printf("Welcome Admin!\n");
            break;
        case USER:
            printf("Welcome User!\n");
            break;
        case GUEST:
            printf("Welcome Guest!\n");
            break;
        default:
            printf("Invalid Role\n");
            break;
    }
    return 0;
}
