//Q63 Merge two arrays.
#include<stdio.h>
#include<stdlib.h>
int main() {
    int s1,s2;
    int *a1,*a2,*merged_array;
    printf("enter size of first and second array\n");
    scanf("%d %d",&s1,&s2);
    a1=(int *)malloc(s1*sizeof(int));
    a2=(int *)malloc(s2*sizeof(int));
    merged_array=(int *)malloc((s1+s2)*sizeof(int));
    printf("enter elements in first array\n");
    for(int i=0;i<s1;i++){
        scanf("%d",(a1+i));
        merged_array[i] = a1[i];
    }
    printf("enter elements in second array\n");
    for(int i=0;i<s2;i++){
        scanf("%d",(a2+i));
    }
    int m_size = s1;
    for(int i=0;i<s2;i++){
        int exist=0;
        for(int j=i;j<s1;j++){
            if(merged_array[j]==a2[i]){
                exist=1;
                break;
            }
        }
        if(!exist) {
            merged_array[m_size++] = a2[i];
        }
    }
    for (int i = 0; i < s1 + s2; i++) {
        printf("%d ", merged_array[i]);
    }
    free(a1);
    free(a2);
    free(merged_array);
    return 0;
}

//Q64: Find the digit that occurs the most times in an integer number.
#include <stdio.h>
int main() {
    long long num;
    int count[10] = {0};

    printf("Enter an integer: ");
    scanf("%lld", &num);

    if (num < 0) num = -num;
    while (num > 0) {
        int digit = num % 10;
        count[digit]++;
        num /= 10;
    }
    int maxDigit = 0;
    for (int i = 1; i < 10; i++) {
        if (count[i] > count[maxDigit]) {
            maxDigit = i;
        }
    }
    printf("%d\n", maxDigit);
    return 0;
}
