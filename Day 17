//Q33: Write a program to check if a number is an Armstrong number.
#include <stdio.h>
#include<math.h>
void main(){
    int n,og,digits=0,sum=0;
    printf("enter a number\n");
    scanf("%d",&n);
    og=n;
    while(og>0){
        int last_num =og%10;
        og =og/10;
        digits+=1;
    }
    og=n;
    while(og>0){
        int last_num =og%10;
        sum+=pow(last_num,digits);
        og =og/10;
        
    }
    if(n==sum){
        printf("Armstrong\n");
    }
    else{
        printf("Not Armstrong");
    }
        }
        
    //Q34: Write a program to check if a number is prime.
#include <stdio.h>
#include<math.h>
void main(){
    int n;
    printf("enter a number\n");
    scanf("%d",&n);
    int isprime=1;
    for(int i=2;i<=sqrt(n);i++){
        if(n%i==0){
            isprime=0;
            break;
            }
        }
        if(isprime){
            printf("Prime\n");
        }
        else{
            printf("Not Prime\n");
        }
        }
