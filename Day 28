//Q55: Write a program to print all the prime numbers from 1 to n.
#include <stdio.h>
#include<math.h>
void main(){
    int n;
    printf("enter a number till which you want to print prime number\n");
    scanf("%d",&n);
    printf("%d ",1);
    for(int i=2;i<=n;i++){
        int isprime=1;
        for(int j=2;j<=sqrt(i);j++){
            if(i%j==0){
                isprime=0;
                break;
            }
        }
        if(isprime){
            printf("%d ",i);
        }
            }
            
        }

//Q56: Read and print elements of a one-dimensional array.
#include <stdio.h>
void main(){
    int size,arr[1000],*j;
    printf("enter size of array ");
    scanf("%d",&size);
    for(int i=0;i<size;i++){
        printf("enter a number in array\n");
        scanf("%d",&arr[i]);
    }
    j=&arr[0];
    for(int i=0;i<size;i++){
        printf("%d ",*j);
        j++;
    }
}
