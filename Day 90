//Q140: Define a struct with enum Gender and print person's gender.
#include <stdio.h>
#include <string.h>
// Define enum for Gender
typedef enum {
    MALE,
    FEMALE,
    OTHER,
    INVALID
} Gender;
// Define struct Person with Gender
typedef struct {
    char name[50];
    Gender gender;
} Person;
// Function to convert string to enum
Gender getGender(char *gStr) {
    if (strcmp(gStr, "MALE") == 0) return MALE;
    else if (strcmp(gStr, "FEMALE") == 0) return FEMALE;
    else if (strcmp(gStr, "OTHER") == 0) return OTHER;
    else return INVALID;
}
int main() {
    char gStr[20];
    scanf("%s", gStr);   // Input format: Gender=MALE
    // Extract actual value after "Gender="
    char *value = strchr(gStr, '=');
    if (value != NULL) value++;  // move past '='
    Person p;
    p.gender = getGender(value);
    switch (p.gender) {
        case MALE:
            printf("Male\n");
            break;
        case FEMALE:
            printf("Female\n");
            break;
        case OTHER:
            printf("Other\n");
            break;
default:
            printf("Invalid Gender\n");
            break;
    }
    return 0;
}
