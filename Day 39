//Q77: Check if the elements on the diagonal of a matrix are distinct.
#include <stdio.h>
int main() {
    int r, c;
    printf("enter rows and columns of matrix\n");
    scanf("%d %d", &r, &c);
    printf("\n");
    int mat[r][c];
    for (int i = 0; i < r; i++)
        for (int j = 0; j < c; j++)
            scanf("%d", &mat[i][j]);

    int isDistinct = 1;
    for (int i = 0; i < r && i < c; i++) {
        for (int j = i + 1; j < r && j < c; j++) {
            if (mat[i][i] == mat[j][j]) {
                isDistinct = 0;
                break;
            }
        }
        if (!isDistinct) break;
    }

    printf("%s\n", isDistinct ? "True" : "False");
    return 0;
}

//Q78: Find the sum of main diagonal elements for a square matrix.
#include <stdio.h>
int main() {
    int n, m;
    printf("enter rows and columns of matrix\n");
    scanf("%d %d", &n, &m);
    printf("\n");

    if (n != m) {
        printf("Not a square matrix\n");
        return 1;
    }

    int mat[n][n];
    for (int i = 0; i < n; i++)
        for (int j = 0; j < n; j++)
            scanf("%d", &mat[i][j]);

    int sum = 0;
    for (int i = 0; i < n; i++)
        sum += mat[i][i];

    printf("%d\n", sum);
    return 0;
}
