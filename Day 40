//Q79: Perform diagonal traversal of a matrix.
#include <stdio.h>
#define MAX 100
int main() {
    int m, n;
    int matrix[MAX][MAX];
    // Read matrix dimensions
    scanf("%d %d", &m, &n);
    // Read matrix elements
    for (int i = 0; i < m; i++)
        for (int j = 0; j < n; j++)
            scanf("%d", &matrix[i][j]);
    // Perform diagonal traversal
    for (int d = 0; d < m + n - 1; d++) {
        if (d % 2 == 0) {
            // Traverse upwards
            for (int i = 0; i <= d; i++) {
                int row = i;
                int col = d - i;
                if (row < m && col < n)
                    printf("%d ", matrix[row][col]);
            }
        } else {
            // Traverse downwards
            for (int i = 0; i <= d; i++) {
                int row = d - i;
                int col = i;
                if (row < m && col < n)
                    printf("%d ", matrix[row][col]);
            }
        }
    }

    return 0;
}

//Q80: Multiply two matrices.
#include <stdio.h>
#define MAX 100
int main() {
    int r1, c1, r2, c2;
    int A[MAX][MAX], B[MAX][MAX], C[MAX][MAX];
    // Read dimensions of first matrix
    printf("enter rows and columns for first matrix\n");
    scanf("%d %d", &r1, &c1);
    // Read first matrix
    printf("enter elements in first matrix\n");
    for (int i = 0; i < r1; i++)
        for (int j = 0; j < c1; j++)
            scanf("%d", &A[i][j]);
    // Read dimensions of second matrix
    printf("enter rows and columns for second matrix\n");
    scanf("%d %d", &r2, &c2);
    // Check if multiplication is possible
    if (c1 != r2) {
        printf("Matrix multiplication not possible\n");
        return 1;
    }
    // Read second matrix
    printf("enter elements in first matrix\n");
    for (int i = 0; i < r2; i++)
        for (int j = 0; j < c2; j++)
            scanf("%d", &B[i][j]);
    // Initialize result matrix to 0
    for (int i = 0; i < r1; i++)
        for (int j = 0; j < c2; j++)
            C[i][j] = 0;
    // Multiply matrices
    for (int i = 0; i < r1; i++) {
        for (int j = 0; j < c2; j++) {
            for (int k = 0; k < c1; k++) {
                C[i][j] += A[i][k] * B[k][j];
            }
        }
    }
    // Print result matrix
    for (int i = 0; i < r1; i++) {
        for (int j = 0; j < c2; j++) {     printf("%d ", C[i][j]);
        }
        printf("\n");
    }
    return 0;
}
