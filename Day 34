//Q67: Insert an element in an array at a given position.
#include <stdio.h>
int main() {
    int n, pos, num;
    scanf("%d", &n);

    int arr[n + 1]; 

    for (int i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    scanf("%d %d", &pos, &num);

    for (int i = n; i > pos; i--)
        arr[i] = arr[i - 1];

    arr[pos] = num;
    n++;

    for (int i = 0; i < n; i++)
        printf("%d ", arr[i]);

    printf("\n");
    return 0;
}

//Q68: Delete an element from an array.
#include <stdio.h>
int main() {
    int n, pos;
    scanf("%d", &n);

    int arr[n];
    for (int i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    scanf("%d", &pos); 

    if (pos < 0 || pos >= n) {
        printf("Invalid position\n");
        return 1;
    }

    for (int i = pos; i < n - 1; i++)
        arr[i] = arr[i + 1];

    n--; 

    for (int i = 0; i < n; i++)
        printf("%d ", arr[i]);

    printf("\n");
    return 0;
}
