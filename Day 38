//Q75: Add two matrices.
#include <stdio.h>
int main() {
    int r1, c1, r2, c2;
    printf("enter rows and columns for first matrix\n");
    scanf("%d %d", &r1, &c1);

    int mat1[r1][c1];
    for (int i = 0; i < r1; i++)
        for (int j = 0; j < c1; j++)
            scanf("%d", &mat1[i][j]);

    printf("\n");

    printf("enter rows and columns for second matrix\n");
    scanf("%d %d", &r2, &c2);
    int mat2[r2][c2];
    for (int i = 0; i < r2; i++)
        for (int j = 0; j < c2; j++)
            scanf("%d", &mat2[i][j]);

    printf("\n");

    if (r1 != r2 || c1 != c2) {
        printf("Matrix dimensions must match.\n");
        return 1;
    }

    int result[r1][c1];
    for (int i = 0; i < r1; i++) {
        for (int j = 0; j < c1; j++) {
            result[i][j] = mat1[i][j] + mat2[i][j];
            printf("%d ", result[i][j]);
        }
        printf("\n");
    }

    return 0;
}

//Q76: Check if a matrix is symmetric.
#include <stdio.h>
int main() {
    int r, c;
    printf("enter rows and columns for matrix\n");
    scanf("%d %d", &r, &c);
    printf("\n");
    int mat[r][c];
    for (int i = 0; i < r; i++)
        for (int j = 0; j < c; j++)
            scanf("%d", &mat[i][j]);

    if (r != c) {
        printf("False\n");
        return 0;
    }

    int isSymmetric = 1;
    for (int i = 0; i < r && isSymmetric; i++) {
        for (int j = 0; j < c; j++) {
            if (mat[i][j] != mat[j][i]) {
                isSymmetric = 0;
                break;
            }
        }
    }
    printf("%s\n", isSymmetric ? "True" : "False");
    return 0;
}
